{
	"name": "portaleargo-api",
	"version": "1.0.0",
	"description": "A library to interact with the api of ArgoScuolaNext",
	"exports": {
		"./web": "./dist/web.js",
		".": "./dist/index.js"
	},
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup && tsc",
		"lint": "eslint src --fix",
		"prettier": "prettier --write src/**/*.ts",
		"test": "npm run test:cli && cross-env NODE_ENV=development tsup && node --enable-source-maps dist/_test.js",
		"test:cli": "eslint src && tsc --noEmit",
		"postinstall": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DTrombett/portaleargo-api.git"
	},
	"author": "DTrombett",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DTrombett/portaleargo-api/issues"
	},
	"homepage": "https://github.com/DTrombett/portaleargo-api#readme",
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"http-cookie-agent": "^7.0.3",
		"tough-cookie": "^6.0.0",
		"undici": "^7.22.0"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@tsconfig/node20": "^20.1.9",
		"@tsconfig/strictest": "^2.0.8",
		"@types/node": "^20.19.33",
		"cross-env": "^10.1.0",
		"dotenv": "^17.3.1",
		"eslint": "^10.0.0",
		"prettier": "^3.8.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0"
	},
	"optionalDependencies": {
		"ajv": "^8.18.0"
	}
}
